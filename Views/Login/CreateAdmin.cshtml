@model Login
@{
    ViewData["Title"] = "CreateAdmin";
    Layout = "~/Views/Layout/Index.cshtml";
}


<div class="content">
    <div class="container-fluid">
        <h4 class="page-title">Admin Listesi</h4>
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header">
                        <div class="card-title">Admin Listesi</div>
                    </div>
                    <div class="card-body">
                        <form method="post" asp-action="CreateAdmin">


                            <label>Kullanıcı Adı</label>
                            <input type="text" asp-for="Username" class="form-control" />

                            <br />

                            <label>Şifre</label>
                            <input type="password" asp-for="Password" class="form-control" />

                            <br />

                         
                            <button class="btn btn-primary">Admini Ekle</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


